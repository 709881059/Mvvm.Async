{
  "version": "1.0.0-eta-05",
  "authors": [ "Stephen Cleary" ],
  "description": "Asynchronous helper types for MVVM applications.",
  "buildOptions": {
    "warningsAsErrors": true,
    "xmlDoc": true
  },
  "packOptions": {
    "tags": [ "async", "mvvm" ],
    "projectUrl": "https://github.com/StephenCleary/Mvvm.Async",
    "licenseUrl": "https://raw.githubusercontent.com/StephenCleary/Mvvm.Async/master/LICENSE",
    "repository": {
      "type": "git",
      "url": "https://github.com/StephenCleary/Mvvm.Async.git"
    }
  },
  "frameworks": {
    "netstandard1.0": {
      "dependencies": {
        "System.ObjectModel": "4.0.12",
        "System.Runtime": "4.1.0",
        "System.Threading.Tasks": "4.0.11"
      }
    },
    ".NETPortable,Version=v4.5,Profile=Profile259": {
      "frameworkAssemblies": {
        "System.ObjectModel": { "type": "build" },
        "System.Runtime": { "type": "build" },
        "System.Threading.Tasks": { "type": "build" }
      }
    }
  },
  "dependencies": {
    "Nito.Mvvm.Core": "1.2.0-alpha-03",
    "Nito.Disposables": "1.1.0-alpha-02"
  }
}
